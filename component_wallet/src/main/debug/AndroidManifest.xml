<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.jianjunhuang.wallet">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:name="com.jianjunhuang.common_base.base.BaseApplication"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:usesCleartextTraffic="true"
    android:theme="@style/AppTheme">
    <activity android:name=".WalletActivity">
      <intent-filter>
        <category android:name="android.intent.category.LAUNCHER"/>
        <action android:name="android.intent.action.MAIN"/>
        <action android:name="android.intent.action.VIEW"/>
      </intent-filter>
    </activity>
    <activity android:name=".view.WalletAddActivity"/>
    <activity android:name=".WalletDetailActivity"/>
  </application>
</manifest>
